{
  "compilerOptions": {
    "lib": [
      "ES2022",
      "DOM"
    ],
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true
  }
}